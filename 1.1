#include <bits/stdc++.h>
using namespace std;
long long ucln(long long a, long long b) {
    while(b!=0){
        long long x=a%b;
        a=b;
        b=x;
    }
    return a;
}
bool arr[1000001];
void snt(long long n){
    for (int i=2;i<=n;i++)
        arr[i]=1;
    arr[0]=arr[1]=0;
    for (int i=2;i<=sqrt(n);i++)
    if (arr[i])
        for (int j=2*i;j<=n;j+=i)
            arr[j]=0;
}
int main(){
    long long a,b,u;
    cin>>a>>b;
    snt(ucln(a,b));
    for (int i=ucln(a,b);i>=2;i--)
        if (arr[i]&&ucln(a,b)%i==0) {cout<<i;break;}
    return 0;
}
